{
    "compilerOptions": {
        "strictNullChecks": true,
        "downlevelIteration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "lib": ["ESNext", "DOM"],
        "moduleResolution": "node"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
